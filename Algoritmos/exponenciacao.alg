algoritmo "CALCULAR_VOLUME"
// Função :
// Autor :
// Data : 10/5/2017
// Seção de Declarações 
var
base,resultado,expoente,cont:real
inicio
      escreval("Digite o valor da base:")
      leia (base)
      escreval("Digite o valor do expoente:")
      leia(expoente)
      resultado<-exp(base,expoente)
      escreval(base,"^",expoente,"=", resultado)


fimalgoritmo
