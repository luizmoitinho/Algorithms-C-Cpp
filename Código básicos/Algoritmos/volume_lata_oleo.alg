algoritmo "CALCULAR_VOLUME"
// Função :
// Autor :
// Data : 10/5/2017
// Seção de Declarações 
var
   V,R,A:real
inicio
 escreval("Digite a altura da lata de óleo :")
 leia(A)
 escreval("Digite o raio da lata de óleo:" )
 leia(R)
 V <- 3.14159*(R*R)*A
 escreval("O volume da lata de oléo é:",V)

fimalgoritmo
